#include<stdio.h>
void main()
{

    int num1=0,num2=0,result;
     printf("\n Enter Any  Two Number ");
     scanf("%d%d",&num1,&num2);

      if(num1>num2)
      {
          result=1;
      }
      else
      result=2;

       if(num1==num2)
      {
          result=0;
        }
      switch(result)
       {

           case 0:
               printf("\n Number Are Same ");
               break;
           case 1:
                printf(" \n Number %d Is Greater",num1);
                break;
           case 2:
                printf("\n Number %d  Is Greater ",num2);
                break;
       }
}








